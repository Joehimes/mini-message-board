<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <h2>Messages</h2>
    <ul>
      <% messages.forEach(function(message) { %>
        <li>
          <h3><%= message.user %></h3>
          <p><%= message.text %></p>
          <h6><%= message.added %></h6>
        </li>
      <% }); %>
    </ul>
    <a href="new">New Message</a>
  </body>
</html>
